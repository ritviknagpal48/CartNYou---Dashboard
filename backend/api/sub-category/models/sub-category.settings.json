{
  "kind": "collectionType",
  "collectionName": "sub_categories",
  "info": {
    "name": "SubCategory",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "product_category": {
      "model": "product-categories",
      "via": "sub_categories"
    },
    "sub_sub_categories": {
      "via": "sub_category",
      "collection": "sub-sub-category"
    },
    "commission": {
      "type": "integer",
      "default": 3,
      "min": 0
    },
    "product_details": {
      "collection": "product-details",
      "via": "sub_category"
    },
    "commission_type": {
      "type": "enumeration",
      "enum": [
        "direct_value",
        "percentage"
      ],
      "default": "percentage"
    }
  }
}
