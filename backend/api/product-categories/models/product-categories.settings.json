{
  "kind": "collectionType",
  "collectionName": "product_categories",
  "info": {
    "name": "productCategories",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "categoryName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "CategoryImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "product_details": {
      "collection": "product-details",
      "via": "product_category"
    },
    "sub_categories": {
      "collection": "sub-category",
      "via": "product_category"
    },
    "commision": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "comission_type": {
      "type": "enumeration",
      "enum": [
        "direct_value",
        "percent_value"
      ],
      "default": "direct_value"
    }
  }
}
