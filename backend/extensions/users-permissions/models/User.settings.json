{
  "kind": "collectionType",
  "collectionName": "users-permissions_user",
  "info": {
    "name": "user",
    "description": ""
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "wholesaler",
        "retailer",
        "admin"
      ],
      "required": true,
      "default": "retailer"
    },
    "fullname": {
      "type": "string"
    },
    "mobile": {
      "type": "string"
    },
    "retailer_shopify_channels": {
      "collection": "shopifychannels",
      "via": "retailersdetails"
    },
    "retailer_live_list": {
      "type": "component",
      "repeatable": true,
      "component": "retailer-product-info.retailer-product"
    },
    "wallet": {
      "type": "float",
      "default": 0,
      "min": 0
    },
    "transaction_history": {
      "type": "component",
      "repeatable": true,
      "component": "transaction-history.transactions"
    },
    "wholesaler_products": {
      "collection": "product-details",
      "via": "wholesaler_details"
    },
    "retailer_import_list": {
      "collection": "product-details",
      "via": "imported_by"
    },
    "warehouses": {
      "via": "user",
      "collection": "warehouse"
    },
    "delivery_requests": {
      "via": "wholesaler_id",
      "collection": "delivery-requests"
    }
  }
}
